# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'  # This is saying that my app is going to run ios 9.0 or later

target 'Flash Chat iOS13' do
  use_frameworks!

  # Pods for Flash Chat iOS13. Type list of pods to install. Then go to terminal and use "pod install"

pod 'Firebase/Auth'
pod 'Firebase/Firestore'
end
